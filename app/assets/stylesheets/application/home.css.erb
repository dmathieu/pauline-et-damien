#home {
  height: 100%;
}

#home .img_us {
  background-image: url('<%= asset_path('nous.jpg') %>');
  background-attachment: fixed;
  background-position: center top;
  background-size: cover;

  margin-top: -20px;
  width: 100%;
  height: 100%;
}

#home .stamp {
  background-image: url('<%= asset_path('stamp.jpg') %>');

  margin: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);

  width: 300px;
  height: 150px;
}
